{
  "name": "gold-bond-react-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "classnames": "^2.3.1",
    "eslint": "^7.21.0",
    "firebase": "^8.4.1",
    "jshint": "^2.12.0",
    "node-sass": "^8.0.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-image-magnify": "^2.7.4",
    "react-image-zoom": "^1.3.1",
    "react-material-ui-carousel": "^2.2.1",
    "react-multi-carousel": "^2.6.2",
    "react-player": "^2.9.0",
    "react-scripts": "4.0.3",
    "styled-components": "^5.2.1",
    "url-polyfill": "^1.1.12",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-app-rewired build && npm run build-rename",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build-rename": "npm run build-rename-js && npm run build-rename-css",
    "build-rename-js": "renamer --find \"/main\\.[^\\.]+\\.chunk.js/\" --replace 'main.js' build/static/js/*.js",
    "build-rename-css": "renamer  --find \"/main\\.[^\\.]+\\.css/\" --replace main.css build/static/css/*.css"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "react-app-rewired": "^2.1.8",
    "renamer": "^2.0.1"
  }
}
